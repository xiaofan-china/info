@charset "utf-8";
.table-list{border:1px solid #D4D4D4;margin-bottom: 10px;box-shadow:0 1px #fff;}
.table-list li{height: 30px;line-height: 30px;border-bottom:1px solid #D4D4D4;}
.table-list li:nth-child(odd){background-color:#F1F1F1;}
.table-list li:nth-child(even){background-color:#fff;}
.table-list li:first-child{font-weight:bold;}
.table-list li:last-child{border:none;}
.table-list li span{float:left;width:11%;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;border-right:1px solid #ccc;text-indent:10px;}
.table-list li span:first-child{padding-left: 12px}
.table-list li span.last{border-right:none;position:relative;}
.table-list li span a{color:#417eb7;}
.table-list li span a:hover{color:#f00;}
.browser-support{padding-top:20px;font-weight:bold;font-size:14px;text-align:center;color:#f00;}
.table-footer{padding:0 15px;}
.sum span{font-weight:bold;text-decoration:underline;padding:0 7px;}
.page span,.page a{float:left;vertical-align:middle;line-height: 20px;}
.page a{margin-left: 5px;}
.page span{padding:0 5px;border:1px solid #A3A3A3;text-align:center;margin-left: 5px;background-image:linear-gradient(#EBECEC,#fff);}
.page span.active{border:1px solid #245B8E;background:#417EB7;color:#fff;}
